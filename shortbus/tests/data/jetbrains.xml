<templateSet group="Djaneiro: Models">
    <template
            name="mauto"
            value="$VAR$ = models.AutoField($VAR2$)"
            toReformat="false"
            toShortenFQNames="true"
    >
        <variable
                name="VAR"
                expression=""
                defaultValue="&quot;FIELDNAME&quot;"
                alwaysStopAt="true"
        />
        <variable
                name="VAR2"
                expression=""
                defaultValue=""
                alwaysStopAt="true"
        />
        <context>
            <option name="Python" value="true" />
        </context>
    </template>
</templateSet>